{
  "expo": {
    "name": "ddn-font-end",
    "slug": "ddn-font-end",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/imageonline-co-convertedpngimage.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "jspackagerOpts": {
      "sourceExts": [
        ".js",
        ".jsx",
        ".ts",
        ".tsx",
        ".android.js",
        ".android.jsx",
        ".ios.js",
        ".ios.jsx"
      ]
    },
    "packagerOpts": {
      "assetExts": [
        ".bmp",
        ".gif",
        ".jpg",
        ".jpeg",
        ".png",
        ".webp",
        "ttf",
        "mp4"
      ]
    },
    "splash": {
      "image": "assets/images/av536-ug3zg.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bayunche.ddnfontend"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/imageonline-co-convertedpngimage.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bayunche.ddnfontend"
    },
    "build": {
      "babel": {
        "include": [
          "@ui-kitten/components"
        ]
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-barcode-scanner",
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "837780ee-f4ed-4e67-ab63-5da3d0409060"
      }
    },
    "owner": "bayunche"
  }

}
